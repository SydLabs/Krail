CREATE TABLE stopTimes (
  trip_id TEXT NOT NULL PRIMARY KEY,
  arrival_time TEXT,
  departure_time TEXT,
  stop_id TEXT,
  stop_sequence INTEGER,
  stop_headsign TEXT,
  pickup_type INTEGER,
  drop_off_type INTEGER
);

INSERT INTO stopTimes (trip_id,arrival_time,departure_time,stop_id,stop_sequence,stop_headsign,pickup_type,drop_off_type)
VALUES (?, ?, ?, ?, ?, ?, ?, ?);
